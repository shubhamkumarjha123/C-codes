//2nd METHORD
 #include <stdio.h>
 int main()
 {
     int i,a,b=0;
    printf("enter the no which table you want to print \n");
    scanf("%d",&a);
    for (i=1;i<=10;i++)
      {
        printf("%d * %d  = %d \n",a,i,b=a*i);
      }   
    return 0;
 }
 
